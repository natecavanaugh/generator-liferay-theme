{
	"name": "<%= themeDirName %>",
	"version": "<%= packageVersion %>",
	"main": "package.json",
	"keywords": [
		"liferay-theme"
	],
	"liferayTheme": {
		"baseTheme": "styled",
		"screenshot": "",
		"rubySass": <%= rubySass %>,
		"templateLanguage": "<%= templateLanguage %>",
		"version": "<%= liferayVersion %>"
	},
	"devDependencies": {
		"gulp": "^3.8.10",
		"liferay-theme-tasks": "^1.4.2",<% if (liferayVersion == 6.2) { %>
		"liferay-theme-deps-6.2": "*"<% } else { %>
		"liferay-theme-deps-7.0": "*"<% } %><% if (liferayVersion == 6.2 && !rubySass) { %>,
		"gulp-sass": "*"<% } else if (liferayVersion == 7.0 && rubySass) { %>,
		"gulp-ruby-sass": "*"<% } %>
	}<% if (publishTag) { %>,
	"publishConfig": {
		"tag": "<%= publishTag %>"
	}<% } %>
}
